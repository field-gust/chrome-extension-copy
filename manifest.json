{
    "manifest_version": 2,
    "name": "Normal Copy Extension",
    "version": "1.0",
    "description": "普通的复制扩展",
    "homepage_url": "https://github.com/lost-manul",
    "background": {
        "page": "background.html"
    },
    "icons": {
        "16": "img/copy.png",
        "48": "img/copy.png",
        "128": "img/copy.png"
    },
    "browser_action": {
        "default_icon": "img/copy.png",
        "default_title": "普通的复制扩展"
    },
    "permissions": [
        "contextMenus"
    ]
}